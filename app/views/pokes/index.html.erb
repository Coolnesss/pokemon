<p id="notice"><%= notice %></p>

<h1>Pokemon</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <table class="table table-hover">
      <thead>
        <tr>
          <th>Name</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @pokemons.each do |poke| %>
        <tr>
          <td><%= link_to poke.name, poke %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
<br>
